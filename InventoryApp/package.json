    "cordova-plugin-splashscreen": "^4.1.0",
    "cordova-plugin-statusbar": "git+https://github.com/apache/cordova-plugin-statusbar.git",
    "cordova-plugin-whitelist": "^1.3.3",
    "font-awesome": "^4.7.0",
    "ionic-angular": "3.7.1",
